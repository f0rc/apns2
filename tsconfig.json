{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "lib": [
            "esnext"
        ],
        "types": [
            "@cloudflare/workers-types"
        ],
        "moduleResolution": "bundler",
        "allowImportingTsExtensions": true,
        "strict": true,
        "noEmit": true,
        "isolatedModules": true,
        "allowJs": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}